@prefix :      <http://opcfoundation.org/UA/2011/03/UANodeSet.xsd#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dtype: <http://www.srdc.com.tr/ontmalizer#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

:attrAlias  a   owl:DatatypeProperty .

:NodeIdDatatype  a      rdfs:Datatype ;
        owl:onDatatype  xsd:string .

:NodeIdAlias  a          owl:Class ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:string ;
                           owl:onProperty     :attrAlias
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:int ;
                           owl:onProperty      :attrAlias
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  :NodeIdDatatype ;
                           owl:onProperty     dtype:hasValue
                         ] .
